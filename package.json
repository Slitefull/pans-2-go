  "scripts": {
    "start": "react-app-rewired start",
    "start:customer": "copy src\\index-customer.tsx src\\index.tsx && npm start",
    "start:admin": "copy src\\index-admin.tsx src\\index.tsx && npm start",
    "build": "npm run convert-locales && npm run build-sw && npm run build-icon-font && npm run copy-flags && react-app-rewired build",
    "build:dev": "env-cmd -f env/.env.dev npm run build",
    "build-sw": "node ./scripts/sw-build.js",
    "build:customer": "copy src\\index-customer.tsx src\\index.tsx && npm run build && rm -rf build-customer && mv build build-customer",
    "build:admin": "copy src\\index-admin.tsx src\\index.tsx && npm run build && rm -rf build-admin && mv build build-admin",
    "rewiredBuild": "react-app-rewired build",
    "test": "react-app-rewired test",
    "coverage": "react-app-rewired test --coverage --watchAll=false",
    "build-icon-font": "rm -rf public/fonts/marketplug-icons && mkdir public/fonts/marketplug-icons && fantasticon",
    "copy-flags": "rm -rf ./public/flags && mkdir ./public/flags && copy -R ./node_modules/svg-country-flags/png250px/ ./public/flags/",
    "eject": "react-app-rewired eject",
    "convert-locales": "ts-node ./scripts/build-translations.ts",
    "serve": "serve -l 8080 -s build",
    "sourceMap": "source-map-explorer 'build/static/js/*.js'",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint:fix": "npm run lint --fix",
    "wsServerDemo": "node scripts/webSocketDemo/wsServer.js"
  },
  "eslintConfig": {
